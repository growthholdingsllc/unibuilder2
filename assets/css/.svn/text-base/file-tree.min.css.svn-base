.file-tree ol.tree li.file.mjs-nestedSortable-no-nesting>div>span:before {
    padding-left: 15px;
    content: "";
	background:url(../images/folder_open.png) no-repeat;
	width:16px;
	height:16px;
	float:left;
	margin: 0px 0px 0 0;
}
.file-tree ol.tree li.folder ol li.mjs-nestedSortable-no-nesting>div>span:before,.file-tree ol.tree li.folder ol li.mjs-nestedSortable-no-nesting>div>a>span:before {
    padding-left: 15px;
    content: "";
	background:url(../images/file_picture.png) no-repeat;
	width:16px;
	height:16px;
	float:left;
	margin: 0px 0px 0 0;
}
.file-tree ol.tree li.file.selected>div {
    color: #fff;
    background: #428bca
}
.file-tree ol.tree li.mjs-nestedSortable-collapsed>ol {
    display: none
}
.file-tree.sortable ol.tree li.file>div>a>span,
.file-tree.sortable ol.tree li.file>div>span,
.file-tree.sortable ol.tree li.folder>div>span {
    cursor: move
}