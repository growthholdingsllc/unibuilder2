}
.et_pb_pricing li {
    line-height: 24px;
    padding: 0 0 10px 14px;
    position: relative;
}
.et_pb_pricing_table_button{
    border: 2px solid;
    color: #00cc00 !important;
	background:transparent;
	border:2px solid;
	border-radius:3px;
	font-size:20px;
	font-weight:500;
	line-height:1.7em;
	padding:6px 20px;
	position:relative;
	transition:all 0.2s;
	margin-top:50px;
}
.et_pb_pricing_table_button:after{
    content: "";
	background:url('../images/arrow_green.png') no-repeat;
    width:20px;
	height:33px;
	float:right;
    margin-left: -20px;
    opacity: 0;
    position: absolute;
    right: 5px;
    top: 7px;
    transition: all 0.2s ease 0s;
}
.et_pb_pricing_table_button:hover:after{
    content: "";
	background:url('../images/arrow_green.png') no-repeat;
    font-size: 32px;
    line-height: 1em;
    margin-left: -20px;
    opacity: 1;
    position: absolute;
    right: 5px;
    top: 7px;
    transition: all 0.2s ease 0s;
}
.et_pb_pricing_table_button.active:after{
    content: "";
	background:url('../images/arrow_green.png') no-repeat;
    font-size: 32px;
    line-height: 1em;
    margin-left: -20px;
    opacity: 1;
    position: absolute;
    right: 5px;
    top: 7px;
    transition: all 0.2s ease 0s;
}
.et_pb_pricing_table_button.active{
	background: rgba(0, 0, 0, 0.05);
    border: 2px solid transparent;
    padding: 6px 34px 6px 14px !important;
	margin-left:0px;
	opacity:1px;
}
.et_pb_pricing_table_button:hover{
    background: rgba(0, 0, 0, 0.05);
    border: 2px solid transparent;
    padding: 6px 34px 6px 14px !important;
	margin-left:0px;
	opacity:1px;
}
