.ui-spinner input {
    border: 1px solid #ccc;
    border-radius: 4px 0 0 4px;
    height: 28px;
    padding: 4px 12px;
}
.ui-spinner-button{
	cursor:pointer;
}
.ui-spinner-up {
    border-left: 1px solid #ccc;
    border-radius: 0 4px 0 0;
    border-right: 1px solid #ccc;
    border-top: 1px solid #ccc;
    float: right;
    height: 14px;
    margin: 0 0 0 -19px;
    padding: 0 2px;
}
.ui-spinner-down {
    border: 1px solid #ccc;
    float: right;
    height: 14px;
    margin: 14px 0 0 -1px;
    padding: 0 2px;
	 border-radius: 0 0px 4px 0;
}
